{
	"name": "minifyall",
	"displayName": "MinifyAll",
	"description": "Minifier for JSON, CSS, HTML, SCSS, and JSONC. you will love its simplicity!",
	"version": "0.5.7",
	"license": "SEE LICENSE IN LICENSE",
	"author": "Jose Gracia Berenguer <jgracia9988@gmail.com>",
	"publisher": "josee9988",
	"icon": "icon-min.png",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other",
		"Formatters",
		"Programming Languages"
	],
	"keywords": [
		"minifier",
		"formatter",
		"format",
		"compresser",
		"compress",
		"minify",
		"css",
		"json",
		"html",
		"scss",
		"jsonc",
		"size",
		"reduce"
	],
	"galleryBanner": {
		"color": "#1b1a1a",
		"theme": "dark"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Josee9988/MinifyAll.git"
	},
	"activationEvents": [
		"onCommand:extension.MinifyAll",
		"onCommand:extension.MinifyAll2OtherDoc",
		"*"
	],
	"main": "./main.js",
	"contributes": {
		"commands": [{
				"command": "extension.MinifyAll",
				"title": "Minify this document ⚡"
			},
			{
				"command": "extension.MinifyAll2OtherDoc",
				"title": "Minify this document and preserve the original ⛏"
			}
		],

		"keybindings": [{
				"command": "extension.MinifyAll",
				"key": "Control+Alt+M M",
				"mac": "Control+Alt+M M",
				"win": "Control+Alt+M M",
				"linux": "Control+Alt+M M",
				"when": "editorTextFocus && editorLangId == css || editorLangId == json || editorLangId == scss || editorLangId == jsonc || editorLangId == html"
			},
			{
				"command": "extension.MinifyAll2OtherDoc",
				"key": "Control+Alt+M N",
				"mac": "Control+Alt+M N",
				"win": "Control+Alt+M N",
				"linux": "Control+Alt+M N",
				"when": "editorTextFocus && editorLangId == css || editorLangId == json || editorLangId == scss || editorLangId == jsonc || editorLangId == html"
			}
		],
		"bugs": {
			"url": "https://github.com/Josee9988/MinifyAll/issues",
			"email": "jgracia9988@gmail.com"
		},
		"homepage": "https://github.com/Josee9988/MinifyAll",
		"scripts": {
			"test": "node ./test/runTest.js"
		},
		"devDependencies": {
			"@types/glob": "^7.1.1",
			"@types/mocha": "^5.2.6",
			"@types/node": "^10.12.21",
			"@types/vscode": "^1.36.0",
			"eslint": "^5.13.0",
			"glob": "^7.1.4",
			"mocha": "^6.1.4",
			"typescript": "^3.3.1",
			"vscode-test": "^1.0.0-next.0"
		}
	}
}