{
	"name": "minifyall",
	"displayName": "MinifyAll",
	"description": "Minifier for JSON, CSS, HTML, LESS, SASS, SCSS, and JSONC. you will love its simplicity!",
	"version": "0.7.0",
	"publisher": "josee9988",
	"license": "SEE LICENSE IN LICENSE",
	"author": "Jose Gracia Berenguer <jgracia9988@gmail.com>",
	"icon": "icon-min.png",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other",
		"Formatters",
		"Programming Languages"
	],
	"keywords": [
		"minifier",
		"formatter",
		"format",
		"compresser",
		"compress",
		"minify",
		"css",
		"json",
		"jsonc",
		"html",
		"less",
		"sass",
		"scss",
		"jsonc",
		"size",
		"reduce"
	],
	"bugs": {
		"url": "https://github.com/Josee9988/MinifyAll/issues",
		"email": "jgracia9988@gmail.com"
	},
	"homepage": "https://github.com/Josee9988/MinifyAll",
	"galleryBanner": {
		"color": "#1b1a1a",
		"theme": "dark"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Josee9988/MinifyAll.git"
	},
	"activationEvents": [
		"onCommand:extension.MinifyAll",
		"onCommand:extension.MinifyAll2OtherDoc",
		"*"
	],
	"main": "./main.js",
	"contributes": {
		"commands": [{
				"command": "extension.MinifyAll",
				"title": "Minify this document ⚡"
			},
			{
				"command": "extension.MinifyAll2OtherDoc",
				"title": "Minify this document and preserve the original ⛏"
			}
		],
		"keybindings": [{
				"command": "extension.MinifyAll",
				"title": "Minify this document ⚡",
				"key": "Ctrl+alt+m",
				"mac": "cmd+alt+m",
				"linux": "Ctrl+alt+m",
				"win": "Ctrl+alt+m"
			},
			{
				"command": "extension.MinifyAll2OtherDoc",
				"title": "Minify this document and preserve the original ⛏",
				"key": "Ctrl+alt+N",
				"mac": "cmd+alt+N",
				"linux": "Ctrl+alt+N",
				"win": "Ctrl+alt+N"
			}
		],
		"configuration": {
			"title": "MinifyAll",
			"properties": {
				"MinifyAll.disableHexadecimalShortener": {
					"type": "boolean",
					"default": false,
					"description": "If you want MinifyAll to stop shortening colors, such as rgb to 3 digit hex, or rgba to hex, or 6 digit hex to 3 digit hex. (True for disabling hex.)"
				},
				"MinifyAll.disableStatusbarInformation": {
					"type": "boolean",
					"default": false,
					"description": "If you want MinifyAll to stop showing an status bar with information about the compression after you minify a file. (True for disabling.)"
				}
			}
		}
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.0-next.0"
	}
}